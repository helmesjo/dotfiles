alias tree='tre --color=automatic'
alias fg1='fg %1'
alias fg2='fg %2'
alias fg3='fg %3'
alias fg4='fg %4'
alias fg5='fg %5'
alias fg6='fg %6'
alias fg7='fg %7'
alias fg8='fg %8'
